#
# Config file in YAML format that controls googleget execution
#
# All variables require an explicit value.
#


#
# friendly_names - map of friendly names to keys
#
:friendly_names:
  "some_friendly_name": "the associated google drive key goes here"


#
# google_drive_user - user name to use with google drive
#
:google_drive_user: "<%= @google_drive_user %>"


#
# google_drive_password - password to use with google drive
#
:google_drive_password: "<%= @google_drive_password %>"
